{
    "name": "simple-react-app-maker",
    "version": "1.1.4",
    "description": "A very simple collection of build scripts for React applications",
    "bin": {
        "simple-react-server": "./server.mjs",
        "simple-react-publish": "./publish.mjs"
    },
    "types": "./index.d.ts",
    "typings": "./index.d.ts",
    "scripts": {
        "format": "prettier --cache --write ."
    },
    "keywords": [
        "react",
        "esbuild"
    ],
    "engines": {
        "node": ">=18"
    },
    "license": "MIT",
    "repository": "https://github.com/TrapTech/simple-react-app-maker",
    "devDependencies": {
        "prettier": "3.2.5",
        "typescript": "5.4.3"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "@esbuild-plugins/node-globals-polyfill": "0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "0.2.2",
        "@types/node": "20.12.5",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "browserslist": "^4.21.4",
        "esbuild": "0.17.19",
        "esbuild-plugin-browserslist": "0.7.2",
        "esbuild-plugin-svgr": "^2.0.0",
        "fs-extra": "11.2.0",
        "jsdom": "24.0.0",
        "node-emoji": "2.1.3",
        "react-csp": "1.1.3"
    }
}
