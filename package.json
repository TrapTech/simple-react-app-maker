{
    "name": "simple-react-app-maker",
    "version": "1.0.1",
    "description": "A very simple collection of build scripts for React applications",
    "bin": {
        "simple-react-server": "./server.mjs",
        "simple-react-publish": "./publish.mjs"
    },
    "types": "./index.d.ts",
    "typings": "./index.d.ts",
    "scripts": {
